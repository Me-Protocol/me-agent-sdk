{
  "name": "me-agent-sdk",
  "version": "1.0.1",
  "description": "AI Chatbot SDK - Embeddable chatbot widget for any website",
  "type": "module",
  "main": "dist/me-agent-sdk.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.cjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:all": "npm run build && npm run build:types",
    "watch": "webpack --watch --config webpack.config.cjs",
    "dev": "npm run watch",
    "serve": "npx serve example -l 5500",
    "dev:full": "concurrently \"npm run watch\" \"npm run serve\"",
    "prepublishOnly": "npm run build:all"
  },
  "keywords": [
    "chatbot",
    "sdk",
    "ai",
    "widget"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@developeruche/protocol-core": "0.10.55-ethers5",
    "@developeruche/runtime-sdk": "0.11.8-development",
    "@gelatonetwork/relay-sdk": "^5.6.0",
    "ethers": "^5.7.2",
    "web3": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "ts-loader": "^9.5.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
